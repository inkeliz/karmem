    "start": "npx serve assemblyscript"
  },
  "repository": {
    "type": "git",
    "url": "github.com/inkeliz/karmem"
  },
  "author": "inkeliz",
  "license": "MIT",
  "devDependencies": {
    "assemblyscript": "0.20.6"
  },
  "exports": {
    ".": {
